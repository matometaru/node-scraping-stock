{
  "name": "node-scraping-stock",
  "version": "1.0.0",
  "main": "main.js",
  "repository": "https://github.com/matometaru/node-scraping-stock.git",
  "author": "Ryosuke",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all build:bundle build:minify build:decls",
    "build:bundle": "rollup -c",
    "build:minify": "uglifyjs -c -m --comments -o index.min.js -- index.js",
    "build:decls": "dts-bundle --baseDir ./ --name index --main decls/index.d.ts",
    "build:watch": "npm run build:bundle -- --watch",
    "eslint": "eslint . --fix",
    "clean": "rm -rf dist/"
  },
  "devDependencies": {
    "catw": "^1.0.1",
    "eslint": "^5.7.0",
    "eslint-config-google": "^0.10.0",
    "http-server": "^0.11.1",
    "try-thread-sleep": "^2.0.0",
    "@types/jest": "^22.2.2",
    "browser-sync": "^2.23.6",
    "cpx": "^1.5.0",
    "dts-bundle": "^0.7.3",
    "gh-pages": "^1.1.0",
    "jest": "^22.4.3",
    "npm-run-all": "^4.1.2",
    "rollup": "^0.57.1",
    "rollup-plugin-typescript2": "^0.12.0",
    "rollup-watch": "^4.3.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "@types/node": "^10.12.0",
    "bl": "^2.1.2",
    "cheerio-httpcli": "^0.7.3",
    "csv": "^4.0.0",
    "iconv-lite": "^0.4.24",
    "lodash": "^4.17.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.spec.ts?(x)"
    ]
  }
}
